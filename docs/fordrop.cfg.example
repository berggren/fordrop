[fordrop]
username = user
api-key = secret
base-url = http://127.0.0.1:8080
verify_ssl = true
verbose = false

[fordrop-plugin-fordropweb]
username = user
api_key = secret

[fordropd]
jid = john@example.org
password = secret
server = xmpp.example.org
pubsub_service = pubsub.example.org
verbose = false
log = false
priority = 127
plugins = print_to_stdout

[fordrop-rest-server]
username = user
api-key = secret
jid = john@example.org
password = secret
server = xmpp.example.org
pubsub_service = pubsub.example.org
bind_address = 127.0.0.1
bind_port = 8080

